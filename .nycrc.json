{
  "include": [
    "src/**/*.js"
  ],
  "exclude": [
    "node_modules/**",
    "test-output/**",
    "tests/**"
  ],
  "reporter": [
    "text",
    "html",
    "lcov"
  ],
  "report-dir": "coverage",
  "all": true,
  "check-coverage": true,
  "lines": 80,
  "functions": 80,
  "branches": 70,
  "statements": 80
}
